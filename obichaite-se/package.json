{
  "name": "obichaite-se",
  "author": {
    "name": "PlanZ"
  },
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "seed:subcategories": "payload run scripts/sub-categories.ts",
    "seed:products": "payload run scripts/products.ts",
    "manual:sitemap": "next-sitemap --config next-sitemap.config.cjs"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.60.0",
    "@payloadcms/email-nodemailer": "3.60.0",
    "@payloadcms/live-preview-react": "3.60.0",
    "@payloadcms/next": "3.60.0",
    "@payloadcms/payload-cloud": "3.60.0",
    "@payloadcms/plugin-form-builder": "3.60.0",
    "@payloadcms/plugin-redirects": "3.60.0",
    "@payloadcms/plugin-seo": "3.60.0",
    "@payloadcms/richtext-lexical": "3.60.0",
    "@payloadcms/ui": "3.60.0",
    "@react-email/components": "^0.5.6",
    "@reduxjs/toolkit": "^2.9.0",
    "@stripe/react-stripe-js": "^5.4.0",
    "@stripe/stripe-js": "^8.5.2",
    "@tailwindcss/typography": "^0.5.16",
    "basic-ftp": "^5.0.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.18",
    "dotenv": "16.4.7",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "next": "15.4.8",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^7.0.9",
    "payload": "3.60.0",
    "react": "19.1.2",
    "react-dom": "19.1.2",
    "react-redux": "^9.2.0",
    "sharp": "0.34.2",
    "stripe": "^20.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@testing-library/react": "16.3.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
